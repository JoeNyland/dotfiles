[user]
    email = joenyland@me.com
    name = Joe Nyland
[push]
    default = simple
[core]
    autocrlf = input
[help]
    autocorrect = 1
[fetch]
    prune = true
[rebase]
    autosquash = true
[filter "lfs"]
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
[color]
    ui = true
[pull]
	ff = only
[branch]
	autoSetupRebase = remote
